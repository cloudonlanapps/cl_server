{
  "typeCheckingMode": "strict",
  "include": [
    "services/packages/cl_ml_tools/src",
    "services/auth/src",
    "services/shared/src",
    "services/store/src",
    "services/compute/src",
    "sdks/pysdk/src"
  ],
  "exclude": [
    "**/__pycache__",
    "**/node_modules",
    "**/.venv",
    "**/build",
    "**/dist"
  ],
  "reportCallInDefaultInitializer": "none",
  "executionEnvironments": [
    {
      "root": "services/packages/cl_ml_tools",
      "extraPaths": [
        "src",
        "typings"
      ]
    },
    {
      "root": "services/auth",
      "extraPaths": [
        "services/auth/src"
      ]
    },
    {
      "root": "services/shared",
      "extraPaths": [
        "services/shared/src"
      ]
    },
    {
      "root": "services/store",
      "extraPaths": [
        "services/store/src",
        "services/store/typings"
      ]
    },
    {
      "root": "services/compute",
      "extraPaths": [
        "services/compute/src"
      ]
    },
    {
      "root": "sdks/pysdk",
      "extraPaths": [
        "sdks/pysdk/src"
      ]
    },
    {
      "root": "."
    }
  ]
}